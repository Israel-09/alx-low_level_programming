#include "main.h"
#include <stdio.h>

/**
 * print_to_98 - prints all integers from n to 98
 */
void print_to_98(int n)
{
	int digit; 
	int i;

	if (n > 98)
	{
		for (; n >= 98; n--)
		{
			if (n > 100)
			{
				_putchar(n / 100 + '0');
			}
			_putchar(n / 10 + '0');
			_putchar(n % 10 + '0');
			if (n != 98)
			{
				_putchar(',');
				_putchar(' ');
			}
			
		}
	}
	_putchar('\n');
}
